              "$ref": "#/definitions/binaryAnswer"
            },
            "workerProvideEquipment": {
              "$ref": "#/definitions/binaryAnswer"
            }
          },
          "minProperties": 0,
          "additionalProperties": false
        },
        "financialRiskB": {
          "type": "object",
          "properties": {
            "workerProvideConsumablesMaterials": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "engagerPayExpense": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerNeedEquipment": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerProvideEquipment": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerPaidInclusive": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "provideVehicle": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerIncurExpense": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerMainIncome": {
              "type": "string",
              "enum": [
                "incomeCalendarPeriods",
                "incomePieceRate",
                "incomeCommission",
                "incomeFixed",
                "incomeProfitOrLosses",
                "incomeRateByInvoice"
              ]
            },
            "WorkerSufferedLatePayment": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerProvideAtTheirExpense": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerFixAtTheirOwnTime": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerPayForMaterialsSubstandardWorker": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "paidForSubstandardWork": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "actualEngagerWithholdPayment": {
              "$ref": "#/definitions/binaryAnswer"
            }
          }
        },
        "partAndParcel": {
          "type": "object",
          "properties": {
            "workerReceivesBenefits": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerAsLineManager": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "contactWithEngagerCustomer": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerRepresentsEngagerBusiness": {
              "$ref": "#/definitions/binaryAnswer"
            }
          },
          "minProperties": 0,
          "additionalProperties": false
        },
        "businessStructure": {
          "type": "object",
          "properties": {
            "similarWork": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerVAT": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "businesAccount": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "advertiseForWork": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "businessWebsite": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerPayForTraining": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerExpenseRunningBusinessPremises": {
              "$ref": "#/definitions/binaryAnswer"
            },
            "workerPaysForInsurance": {
              "$ref": "#/definitions/binaryAnswer"
            }
          },
          "minProperties": 0,
          "additionalProperties": false
        }
      },
      "minProperties": 0,
      "additionalProperties": false
    }
  },
  "required": [
    "version",
    "correlationID",
    "interview"
  ],
  "additionalProperties": false
}