{
  "id": "https://raw.githubusercontent.com/hmrc/off-payroll-decision/master/test/resources/schema/off-payroll-response-schema.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Decision Response Version 1.0.0-beta",
  "definitions": {
    "weightedAnswer": {
      "type": "string",
      "enum": [
        "HIGH",
        "LOW"
      ]
    }
  },
  "type": "object",
  "properties": {
    "version": {
      "type": "string",
      "pattern": "^[0-9]{1,2}[.][0-9]{1,2}[.][0-9]{1,2}-?[a-zA-Z-_]*$"
    },
    "correlationID": {
      "type": "string"
    },
    "result": {
      "type": "string",
      "enum": [
        "Outside IR35",
        "Self-Employed",
        "Inside IR35",
        "Employed",
        "Unknown"
      ]
    },
    "score": {
      "type": "object",
      "properties": {
        "personalService": {
          "$ref": "#/definitions/weightedAnswer"
        },
        "control": {
          "$ref": "#/definitions/weightedAnswer"
        },
        "financialRiskA": {
          "$ref": "#/definitions/weightedAnswer"
        },
        "financialRiskB": {
          "$ref": "#/definitions/weightedAnswer"
        },
        "partAndParcel": {
          "$ref": "#/definitions/weightedAnswer"
        },
        "businessStructure": {
          "$ref": "#/definitions/weightedAnswer"
        }
      },
      "addtionalProperties": false,
      "required": [
        "personalService",
        "control",
        "financialRiskA",
        "financialRiskB",
        "partAndParcel",
        "businessStructure"
      ]
    }
  },
  "required": [
    "version",
    "correlationID",
    "score"
  ],
  "addtionalProperties": false
}